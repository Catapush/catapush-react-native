{
  "name": "catapush-react-native",
  "version": "1.6.0",
  "description": "Catapush",
  "homepage": "https://www.catapush.com/",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Catapush <catapush.com@gmail.com>",
  "license": "Apache-2.0",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace example",
    "compile": "rm -rf lib && tsc -p .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "yarn compile",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.83",
    "@react-native/eslint-config": "0.74.83",
    "@react-native/metro-config": "0.74.83",
    "@react-native/typescript-config": "0.74.83",
    "@types/jest": "^29.5.2",
    "@types/react": "^18.2.6",
    "@types/react-native": "^0.72.2",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "^0.76.7",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "react-native": "0.74.1",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "workspaces": [
    "example"
  ],
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "date-fns": "^2.30.0",
    "prop-types": "^15.8.1"
  },
  "directories": {
    "example": "example",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Catapush/catapush-react-native.git"
  },
  "keywords": [
    "react-native",
    "react-component",
    "ios",
    "android",
    "push",
    "apns",
    "fcm",
    "gcm",
    "catapush"
  ],
  "bugs": {
    "url": "https://github.com/Catapush/catapush-react-native/issues"
  }
}
